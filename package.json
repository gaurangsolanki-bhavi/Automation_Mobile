{
  "name": "ronspot_test_automation",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test:member": "powershell -Command \"npx playwright test Tests/Member/ --reporter=dot | ForEach-Object { $_ -replace '\\e\\[[\\d;]*[A-Za-z]', '' } | Tee-Object -FilePath TestData/Logs/test-execution-$(Get-Date -Format 'yyyy-MM-dd-HH-mm-ss').txt\"",
    "test:admin": "powershell -Command \"npx playwright test Tests/admin/ --reporter=dot | ForEach-Object { $_ -replace '\\e\\[[\\d;]*[A-Za-z]', '' } | Tee-Object -FilePath TestData/Logs/test-execution-$(Get-Date -Format 'yyyy-MM-dd-HH-mm-ss').txt\"",
    "runload": "node ./Tests/LoadTest/LoadUtils/runload.js",
    "test:web": "npx playwright test",
    "test": "node test.js",
    "test:all": "npm run test:web && npm run test:mobile",
    "allure:generate": "allure generate allure-results --clean -o allure-report",
    "allure:open": "allure open allure-report",
    "clean:allure": "rm -rf allure-results allure-report",
    "test:android:sauce": "node test.sauce.js"
  },
  "author": "Akshay Kankodiya",
  "license": "ISC",
